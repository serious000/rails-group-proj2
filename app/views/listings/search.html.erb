<h1>Listings#search</h1>
<p>Find me in app/views/listings/search.html.erb</p>

<% @listings.each do |l| %>
  <div>
    <p><a href="/listings/<%= l.id %>"> <%= l.title %></a></p>
    <p>$<%= l.price %></p>
    <p>(<%= l.city %>)</p>
    <p> posted <%= time_ago_in_words(l.created_at) %> ago</p>
  </div>
  <br><br>
<% end %>
