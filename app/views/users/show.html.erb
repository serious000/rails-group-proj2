<style>

#contain{
	width:40%;
	/*border:.1px solid;*/

	height:600px;
	margin-left:25%;
	overflow: hidden;
}
}
#lists{
	width:50%;
	border: .1px solid;
	margin-left:40%;
	vertical-align: top;
	display: inline-block;
}
</style>
<div id="contain">
	<h1> Welcome <%= current_user.first_name %></h1>
	<h2>Your listings</h2>
	<hr>
		<div id="lists">
	<% @listings.each do |l| %>
		<div>
			<p><a href="/listings/<%=l.id%>"><%= l.title %></a></p>
			<p><%= l.description %></p>
		</div>
		<br><br>
		<% end %>
	</div>
</div>
